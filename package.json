{
  "name": "caitienwebtt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:fast": "NODE_ENV=production next build --no-lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "DATABASE_URL=\"file:./dev.db\" ts-node scripts/seed.ts",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "perf:build": "NODE_ENV=production next build --no-lint --debug",
    "perf:audit": "node scripts/performance-audit.js",
    "bundle:analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "dev:fast": "NODE_ENV=development next dev --turbo",
    "start:fast": "NODE_ENV=production next start --turbo",
    "fix:all": "npm run lint:fix && npm run type-check",
    "clean": "rm -rf .next out dist"
  },
